   "source": [
    "import random\n",
    "\n",
    "count = 0\n",
    "while True:\n",
    "    dice_roll = random.randint(1, 6)\n",
    "    count += 1\n",
    "    if dice_roll == 6:\n",
    "        break\n",
    "\n",
    "print(\"Number of rolls to get a 6:\", count)"
